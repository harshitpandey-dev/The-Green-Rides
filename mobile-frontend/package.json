{
  "name": "mobile-frontend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "clean:android": "cd android && gradlew clean && cd .. && npx rimraf android/build android/app/build",
    "clean:ios": "npx rimraf ios/build ios/Pods && cd ios && pod cache clean --all && cd ..",
    "clean:node": "npx rimraf node_modules && npm cache clean --force",
    "clean:all": "npm run clean:android && npm run clean:ios && npm run clean:node"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-native-community/geolocation": "^3.4.0",
    "@react-native-picker/picker": "^2.11.1",
    "@react-navigation/bottom-tabs": "^7.4.6",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.7",
    "@reduxjs/toolkit": "^2.8.2",
    "axios": "^1.11.0",
    "jwt-decode": "^4.0.0",
    "react": "19.1.0",
    "react-native": "0.81.0",
    "react-native-camera": "^4.2.1",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-image-picker": "^8.2.1",
    "react-native-qrcode-svg": "^6.3.15",
    "react-native-safe-area-context": "^5.6.0",
    "react-native-screens": "^4.14.1",
    "react-native-svg": "^15.12.1",
